{
  "name": "graphql.js",
  "version": "0.6.1",
  "description": "GraphQL Client for JavaScript",
  "main": "dist/graphql.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "files": [
    "dist/graphql.js"
  ],
  "scripts": {
    "doc": "jsdoc --configure .jsdoc.json --verbose",
    "test": "jest",
    "dev": "poi example/main.js --serve",
    "prettier": "prettier --single-quote --write src/**/*.js",
    "build": "poi src/graphql.js --prod --format umd --module-name graphql --file-names.js graphql.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/f/graphql.js.git"
  },
  "keywords": [
